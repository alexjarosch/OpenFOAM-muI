FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.snow 0
);

regions
(
    boxToCell
    {
        box (-0.1 -0.1 -0.1) (0.06 0.2 0.2);
        fieldValues
        (
            volScalarFieldValue alpha.snow 1
        );
    }
);


